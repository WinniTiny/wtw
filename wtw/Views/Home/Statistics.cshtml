
@{
    ViewBag.Title = "Statistics";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<head>
    <style type="text/css">
        .grid-container {
            display: grid;
            height: 100%;
            grid-template-columns: 1.25fr 3.1fr 0.1fr 1.2fr 1.25fr;
            grid-template-rows: 0.1fr 1fr 0.3fr 1fr 0.5fr 1fr;
            grid-gap: 1px 1px;
            grid-template-areas: ". header1 header1 header1 ." ". chart-overall chart-overall chart-overall ." ". header2 header2 header2 ." ". chart-overall-search . chart-overall-searchbar ." ". header3 header3 header3 .";
        }

        .header1 {
            grid-area: header1;
        }

        .chart-overall {
            grid-area: chart-overall;
        }

        .header2 {
            grid-area: header2;
        }

        .chart-overall-search {
            grid-area: chart-overall-search;
        }

        .chart-overall-searchbar {
            grid-area: chart-overall-searchbar;
        }

        .header3 {
            grid-area: header3;
        }

        .chart-map {
            grid-area: chart-map;
        }

        /* set the CSS */

        .bar {
            fill: steelblue;
        }
    </style>

    <style>
        /* set the CSS */

        .bar {
            fill: steelblue;
        }
    </style>

    <link href="/assets/css/stat-bubble.css" rel="stylesheet" type="text/css" />
    <!-- js-->
    <!-- load the d3.js library -->
    <!--End / js-->
</head>

<div style="width:auto;height:8em">
    <img src="~/assets/img/blackbar.png" style="background-size:cover" />
</div>
<!-- Content-->
<div class="md-content">

    <!-- page-title -->
    <div class="page-title">
        <div class="container">
            <div class="row">
                <div class="col-lg-8 offset-0 offset-sm-0 offset-md-0 offset-lg-2 ">
                    <h2 class="page-title__title">@ViewBag.Title</h2>

                    <!--  -->
                    <nav>
                        <a class="breadcrumb__item" href="@Url.Action("Index","Home")">Home</a><span class="breadcrumb__item active">@ViewBag.Title</span>
                    </nav><!-- End /  -->

                </div>
            </div>
        </div>
        <section class="md-section">
            <div class="grid-container">
                <div class="header1">
                    <hr />
                    <div style="margin:2em"></div>
                    <iframe frameborder=0 width="800" height="600" src="https://reports.zoho.com/open-view/1804641000000002397/5d65ba53161787410f83624c6bf54d1b"></iframe>
                </div>
                <div class="chart-overall" id="mainBubble" style="align-items:center">
                </div>

                <div class="header2">
                    <hr />
                    <div style="margin:2em"></div>

                </div>
                <div class="chart-overall-search" id="inputbarchart">
                </div>
                <div class="chart-overall-searchbar">

                </div>
                <div class="header3">
                    <hr />
                    <div style="margin:2em"></div>

                </div>
            </div>

        </section>
        <section style="align-content:center"></section>
        <section class="md-section">
            <div class="text-center"><a class="btn btn-primary btn-w180" href="#">Load more</a></div>
        </section>

    </div><!-- End / page-title -->
</div>
<!-- End / Content-->
<!-- js-->
<!-- load the d3.js library -->
<script src="https://d3js.org/d3.v4.min.js"></script>
<script src="http://code.jquery.com/jquery-1.9.1.min.js"></script>
<!--<script src="/assets/js/stat-bubble.js"></script>-->
<script src="/assets/js/validator.js"></script>
